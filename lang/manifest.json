{
   "background_page": "background.html",
   "options_page": "conf.html",
   "browser_action": {
     "default_title": "Chrowety",
     "default_icon": "icones/x16.png",
     "popup": {
        "path": "popup.html"
     }
   },
   "permissions": [
     "tabs",
     "http://twitter.com/",
     "https://twitter.com/",
     "http://3.ly/*",
     "http://migre.me/*",
     "http://api.bit.ly/*",
     "http://api.j.mp/*",
     "http://is.gd/*",
     "http://api.tr.im/*",
     "http://*.tr.im/*",
     "http://untr.im/*"
   ],
   "icons": {
     "16": "icones/x16.png",
     "32": "icones/x32.png",
     "48": "icones/x48.png",
     "128":"icones/x128.png"
   },
   "name": "Chrowety",
   "description": "A twitter client for Google Chrome",
   "version": "1.6.101",
   "update_url": "http://clients2.google.com/service/update2/crx"
}
